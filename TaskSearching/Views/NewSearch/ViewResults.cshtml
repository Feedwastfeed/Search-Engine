@model IEnumerable<TaskSearching.Models.Result>
@{
    ViewBag.Title = "resultlinks";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>results</h1>
@{if (Model.Count() == 0)
    {<h2>No Match results</h2> } 
    }
<body>
    @{ int count = 1;}
    @foreach (var item in Model)
    {

        <div>
            <td>
                @(count++)---> 
            </td>
            <td>
                <a href="@item.URLS">@item.URLS</a>
            </td>
        </div>
    }
</body>

